{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 120,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSpacing": true,
  "svelteStrictMode": false,
  "svelteSortOrder": "options-scripts-markup-styles",
  "svelteAllowShorthand": true,
  "svelteIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "strict",
  "semi": true,
  "bracketSameLine": false,
  "jsxSingleQuote": true
}
